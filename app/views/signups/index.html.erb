<% content_for :title, "Sign Ups" %>

<table>
  <tr>
	<th>Signup ID</th>
    <th>Coach name</th>
    <th>Coach phone</th>
    <th>Coach email</th>
  </tr>

<% @signups.each do |signup| %>
  <tr>
    <td><%=h signup.id %></td>
    <td><%=h signup.coach_name %></td>
    <td><%=h signup.coach_phone %></td>
    <td><%=h signup.coach_email %></td>
    <td><%= link_to 'Show', signup %></td>
    <td><%= link_to 'Edit', edit_signup_path(signup) %></td>
    <td><%= link_to 'Destroy', signup, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New signup', new_signup_path %>